{
  "name": "ntvstream-stremio-addon",
  "version": "1.0.0",
  "description": "Stremio addon for live sports streaming from NTVStream with multiple server support",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "start:js": "node addon.js",
    "dev": "tsc && node dist/server.js --launch",
    "dev:js": "node addon.js --launch",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "deploy:vercel": "vercel",
    "deploy:beamup": "npx beamup"
  },
  "keywords": [
    "stremio",
    "addon",
    "sports",
    "streaming",
    "football",
    "cricket",
    "ufc",
    "boxing",
    "ppv",
    "live",
    "ntvstream"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "cheerio": "^1.0.0-rc.12",
    "express": "^4.18.2",
    "stremio-addon-sdk": "^1.6.10"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.4",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
